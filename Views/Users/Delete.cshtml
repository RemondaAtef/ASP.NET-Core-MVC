@model ProjectExample.Models.UsersVM

@{
    ViewBag.Title = "Delete";

}

<h2>Delete User</h2>

<form asp-controller="Users" asp-action="Delete" method="post" class="form-group">
   
    <label asp-for="UserName">User Name</label>
    <input type="text" asp-for="UserName" disabled class="form-control" />
    <br />

    <label asp-for="UserCode">User Code</label>
    <input type="text" asp-for="UserCode" disabled class="form-control" />
    <br />

    <center>
        <input type="submit" value="Delete" class="btn btn-primary" />
    </center>
</form>

